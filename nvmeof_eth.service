[Unit]
Description=Connect to all NVMe targets
After=network.target
Before=remote-fs-pre.target
StartLimitBurst=3
StartLimitIntervalSec=10

[Service]
Type=simple
RemainAfterExit=yes
ExecStart=/home/zhongyuling/nvmet/nvme_eth_daemon start
ExecStop=/home/zhongyuling/nvmet/nvme_eth_daemon stop
ExecReload=/home/zhongyuling/nvmet/nvme_eth_daemon reload
Restart=on-failure
RestartSec=3

[Install]
WantedBy=multi-user.target
